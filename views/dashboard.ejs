<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>UniPlan ‚óè Dashboard</title>

    <link rel="icon" type="image/x-icon" href="/public/img/UniPlan Icon.png">
</head>

<body>
    <h1>All Projects</h1>
    <div class="wrapper">
        <% projects.forEach(project => { %>
        <div class="card">
            <h2><%= project.project_name %></h2>
            <p><%= project.project_bio %></p>
            <a href="/project/id=<%= project.project_id %>">View Project</a>
        </div>
        <% }); %>
    </div>

</body>

</html>